{
  "name": "react-base-starter",
  "version": "0.0.1",
  "description": "React Base Starter Kit",
  "author": "Pablo Magaz",
  "private": true,
  "scripts": {
    "tsc": "tsc",
    "logo": "node ./src/base/conf/.react-base/logo.js",
    "clean": "rimraf dist && rimraf .cache",
    "prettify": "prettier --write './src/**/*.{js,ts,tsx}'",
    "lint": "npm run logo && eslint src --ext .ts --ext .js",
    "start": "npm run logo && cross-env NODE_ENV=development node server",
    "start:prod": "npm run build && cross-env NODE_ENV=production node server",
    "build": "npm run logo && npm run clean && cross-env NODE_ENV=production parcel build src/app/index.html -d dist --no-cache",
    "postinstall": "npm run clean"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn build",
      "commit-msg": "echo ⚠️ THINK BEFORE ACTING AND REMEMBER, LESS IS MORE. KISS!! "
    }
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "immutableJS",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "babel-eslint": "10.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "esm": "3.2.25",
    "husky": "2.3.0",
    "immutable": "4.0.0-rc.12",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier-eslint": "8.8.2",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.3",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "12.0.2",
    "@types/react": "16.8.18",
    "@types/react-dom": "16.8.4",
    "axios": "0.18.0",
    "babel-polyfill": "6.26.0",
    "create-reduxreducer": "1.0.0",
    "express": "4.17.1",
    "history": "4.9.0",
    "immer": "3.1.2",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-eslint": "1.0.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "v7.1.0-alpha.5",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-react-hook": "3.3.2",
    "redux-req-middleware": "1.0.4",
    "styled-components": "4.2.0",
    "ts-node": "8.2.0"
  }
}
