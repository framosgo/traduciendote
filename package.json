{
  "name": "react-base-starter",
  "version": "0.0.1",
  "description": "React Base Starter Kit",
  "authors": [
    "Pablo Magaz (@pmagaz)",
    "José Bermudo (@josemanuelbd)",
    "Alberto Martín (@albertoMartin1988)",
    "Rafael Pérez (@rperez.cognizant)"
  ],
  "private": true,
  "scripts": {
    "postinstall": "npm run clean",
    "logo": "ts-node ./src/base/conf/.react-base/logo.ts",
    "clean": "rimraf dist && rimraf .cache",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "check-integrity": "npm run lint && npm run types && npm run test",
    "lint": "npm run logo && eslint src --ext .ts --ext .tsx --ext .js --ext .jsx",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run logo && npm run clean && cross-env NODE_ENV=production parcel build src/app/index.html -d dist --no-cache",
    "start": "npm run logo && cross-env NODE_ENV=development ts-node server",
    "start:prod": "npm run build && cross-env NODE_ENV=production ts-node server"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run types",
      "pre-push": "npm run test && npm run build",
      "commit-msg": "echo ⚠️ THINK BEFORE ACTING AND REMEMBER, LESS IS MORE. KISS!! "
    }
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "immutableJS",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.13",
    "@types/node": "^13.1.6",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.1.15",
    "@types/webpack-env": "1.15.0",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "6.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "esm": "3.2.25",
    "husky": "4.0.6",
    "jest": "^24.8.0",
    "lint-staged": "^9.5.0",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier-eslint": "9.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "rimraf": "3.0.0",
    "typescript": "3.7.4"
  },
  "dependencies": {
    "axios": "0.19.1",
    "core-js": "^3.6.3",
    "create-reduxreducer": "1.0.0",
    "express": "4.17.1",
    "history": "4.10.1",
    "immer": "5.2.1",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-eslint": "1.0.7",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "redux": "4.0.1",
    "redux-react-hook": "3.3.2",
    "redux-req-middleware": "1.0.4",
    "styled-components": "4.4.1",
    "ts-node": "8.6.1"
  },
  "engines": {
    "node": "10.x"
  }
}
