{
  "name": "react-base-starter",
  "version": "0.0.1",
  "description": "React Base Starter Kit",
  "author": "Pablo Magaz",
  "private": true,
  "scripts": {
    "postinstall": "npm run clean",
    "logo": "ts-node ./src/base/conf/.react-base/logo.ts",
    "clean": "rimraf dist && rimraf .cache",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "lint": "npm run logo && eslint src --ext .ts --ext .tsx --ext .js --ext .jsx",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run logo && npm run clean && cross-env NODE_ENV=production parcel build src/app/index.html -d dist --no-cache",
    "start": "npm run logo && cross-env NODE_ENV=development ts-node server",
    "start:prod": "npm run build && cross-env NODE_ENV=production ts-node server"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run types",
      "pre-push": "npm run test && npm run build",
      "commit-msg": "echo ⚠️ THINK BEFORE ACTING AND REMEMBER, LESS IS MORE. KISS!! "
    }
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "immutableJS",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-transform-runtime": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^4.3.3",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "^4.1.15",
    "@types/webpack-env": "1.13.9",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "5.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "eslint-plugin-react-hooks": "1.6.0",
    "esm": "3.2.25",
    "husky": "2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.5",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier-eslint": "8.8.2",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "rimraf": "2.6.3",
    "typescript": "3.4.5"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "12.0.3",
    "@types/parcel-bundler": "^1.12.0",
    "@types/react-dom": "16.8.4",
    "axios": "0.18.0",
    "core-js": "^3.2.1",
    "create-reduxreducer": "1.0.0",
    "express": "4.17.1",
    "history": "4.9.0",
    "immer": "3.1.2",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-eslint": "1.0.7",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "v7.1.0-alpha.5",
    "react-router-dom": "5.0.0",
    "redux": "4.0.1",
    "redux-react-hook": "3.3.2",
    "redux-req-middleware": "1.0.4",
    "styled-components": "4.2.0",
    "ts-node": "8.2.0"
  },
  "engines": {
    "node": "10.x"
  }
}
